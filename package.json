{
  "name": "alfa-ikebukuro-13",
  "version": "0.0.0",
  "scripts": {
    "css": "sass assets/css/style.scss:assets/css/style.css --no-source-map",
    "prefix": "postcss assets/css/style.css --use autoprefixer -d assets/css/",
    "build": "yarn run css && yarn run prefix"
  },
  "browserslist": [
    "> 1% in JP",
    "last 2 version",
    "Firefox ESR",
    "not IE <= 11",
    "not IE_MOB <= 11"
  ],
  "devDependencies": {
    "autoprefixer": "^10.3.4",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "sass": "^1.41.0"
  }
}
